{
  "expo": {
    "name": "pe_de_ouro",
    "slug": "pe_de_ouro",
    "scheme": "pe-de-ouro-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sapatos.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img_telaInicial_app.png",
      "resizeMode": "cover",
      "backgroundColor": "#FEFAE0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img_telaInicial_app.png",
        "backgroundColor": "#FEFAE0"
      }
    },
    "web": {
      "output": "server",
      "favicon": "./assets/img_telaInicial_app.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
